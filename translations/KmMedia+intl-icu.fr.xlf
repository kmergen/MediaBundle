                <source>dashboard.badge_main</source>
                <target>Image principale</target>
            </trans-unit>
            <trans-unit id="dashboard.delete_confirm">
                <source>dashboard.delete_confirm</source>
                <target>Voulez-vous vraiment supprimer cette image ?</target>
            </trans-unit>
            <trans-unit id="dashboard.loading">
                <source>dashboard.loading</source>
                <target>Chargement...</target>
            </trans-unit>
            <trans-unit id="dashboard.error.max_files">
                <source>dashboard.error.max_files</source>
                <target>{count, plural, =1 {Limite atteinte : Un seul fichier autorisé.} other
                    {Limite atteinte : # fichiers maximum autorisés.}}</target>
            </trans-unit>
