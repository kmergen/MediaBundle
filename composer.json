{
  "name": "kmergen/media-bundle",
  "description": "A Symfony bundle for handling media uploads.",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Klaus Mergen",
      "email": "klausmergen1@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "symfony/framework-bundle": "^7.2",
    "symfony/form": "^7.2",
    "doctrine/orm": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Kmergen\\MediaBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kmergen\\MediaBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    }
  }
}
